/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": ["src/app/*"],
      "@environments/*": ["src/app/environments"],
      "@core": ["src/app/core"],
      "@core/*": ["src/app/core/*"],
      "@shared": ["src/app/shared"],
      "@shared/*": ["src/app/shared/*"],
      "@models": ["src/app/models"],
      "@models/*": ["src/app/models/*"],
      "@services": ["src/app/services"],
      "@services/*": ["src/app/services/*"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
