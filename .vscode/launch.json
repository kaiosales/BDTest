    "version": "0.2.0",
    "configurations": [
        {
            "name": "BDTest.EAPI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/BDTest.EAPI/bin/Debug/netcoreapp3.1/BDTest.EAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": false,
            "env": {
                "ASPNETCORE_URLS": "https://localhost:5001;http://localhost:5000",
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "BDTest.IAPI",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/BDTest.IAPI/bin/Debug/netcoreapp3.1/BDTest.IAPI.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "justMyCode": false,
            "env": {
                "ASPNETCORE_URLS": "https://localhost:6011;http://localhost:6010",
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/BDTest.ConsoleClient/bin/Debug/netcoreapp3.1/BDTest.ConsoleClient.exe",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}